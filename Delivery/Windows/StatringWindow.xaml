<Window x:Class="Delivery.Windows.StatringWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Delivery.Windows"
        mc:Ignorable="d"
        Height="450" Width="800" MinHeight="450" MinWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
 
        <Rectangle Fill="#FF71CC76" Grid.Row="0"></Rectangle>
        <Button Content="Назад" x:Name="btnBack" Click="btnBack_Click" HorizontalAlignment="Left" Margin="10 0 0 0"></Button>

        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
</Window>
